<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Flive</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
    <!-- CSS Libs -->
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/animate.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/checkbox3.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/dataTables.bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/lib/css/select2.min.css">
    <link href="~/Content/admin/css/jquery.mCustomScrollbar.css" rel="stylesheet" />
    <link href="~/Content/admin/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Content/admin/css/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/Content/admin/semantic.css" rel="stylesheet" />
    <link href="~/Content/modal.min.css" rel="stylesheet" />
    <link href="~/Content/statistic.min.css" rel="stylesheet" />

    <!-- CSS App -->
    <link rel="stylesheet" type="text/css" href="~/Content/admin/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/admin/css/themes/flat-green.css">
    <link href="~/Content/admin/Site.css" rel="stylesheet" />

    <style>
        .menu-icon {
            margin-left: 3px;
            width: 32px;
            height: 32px;
            text-align: center;
            font-size: 1.1em;
            display: inline-block;
            background-size: contain;
        }
        .home-menu {
            background-image: url('../Content/admin/img/home_icon.png');
        }
        .triner-menu {
            background-image: url('../Content/admin/img/trainer_icon.png');
        }
        .rebook-menu {
            background-image: url('../Content/admin/img/rebook_icon.png');
        }
        .signout-menu {
            background-image: url('../Content/admin/img/rebook_icon.png');
        }
    </style>
</head>
<body class="flat-green landing-body">
    <div class="app-container">
        <div class="row content-container">
            <nav class="navbar navbar-default navbar-fixed-top navbar-top">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-expand-toggle" style="width:30px;">
                            <i style="display:none" class="fa fa-bars icon"></i>
                        </button>
                        <ol class="breadcrumb navbar-breadcrumb">
                            @RenderSection("Breadcrumb", required: false)
                        </ol>
                        <button type="button" class="navbar-right-expand-toggle pull-right visible-xs">
                            <i class="fa fa-th icon"></i>
                        </button>
                    </div>
                    <ul class="nav navbar-nav navbar-right" style="display:">
                        @if (User.Identity.IsAuthenticated)
                        {
                            <a href="~/Home/LogOut" class="btn btn-default" style="margin-top: 12px;margin-right: 20px;"><i class="fa fa-sign-out"></i> Sign out</a>
                        }

                    </ul>
                </div>
            </nav>







            <div class="side-menu sidebar-inverse">
                <nav class="navbar navbar-default" role="navigation">
                    <div class="side-menu-container">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="#">
                                <div style="display: inline-block">
                                    <img class="hedder-logo-light-compact" src="~/Content/admin/img/logo_light.png" />
                                </div>
                                <div class="title" style="display: inline-block">
                                    <img src="~/Content/admin/img/logo_horizontal_colord_web.png" class="hedder-logo-light" />
                                </div>
                            </a>
                            <button type="button" class="navbar-expand-toggle pull-right visible-xs">
                                <i class="fa fa-times icon"></i>
                            </button>
                        </div>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <ul class="nav navbar-nav">
                                <li>
                                    <a href="~/Home/index">
                                        
                                        <div class="menu-icon home-menu"></div><div class="title">Dashboard</div>
                                    </a>
                                </li>
                                <li>
                                    <a href="~/Trainers/index">

                                        <div class="menu-icon triner-menu"></div><div class="title">Manage Trainers</div>
                                    </a>
                                </li>
                                <li>
                                    <a href="~/FreeWorkouts/index">

                                        <div class="menu-icon rebook-menu"></div><div class="title">Free Videos</div>
                                    </a>
                                </li>
                               
                            </ul>
                        }

                    </div>
                    <!-- /.navbar-collapse -->
                </nav>
            </div>



            <!-- Main Content -->
            <div class="container-fluid">
                <div class="side-body">
                    @RenderBody()
                    @RenderSection("SPAViews", required: false)

                </div>
            </div>
        </div>
    </div>

    <div id="DIV_InnerPageContent_Loading" style="display:none;">
        <div style="position: relative;top:150px; width: 200px; left: 50%; margin-left: -100px; text-align: center; padding-bottom: 150px;color: #aaa;font-size: 14px;">
            <img src="~/Content/admin/img/ripple.svg" /><br /><br />
            Loading. Please wait.
        </div>
    </div>
    <div id="DIV_InnerListContent_Loading" style="display:none;">
        <div style="position: relative;width: 200px; left: 50%; margin-left: -100px; text-align: center; padding-bottom: 150px;color: #aaa;font-size: 14px;">
            <img src="~/Content/admin/img/ripple.svg" /><br /><br />
            Loading list. Please wait.
        </div>
    </div>

    <div id="SystemMainAzyncLoadingDiv">
        Loading. Please wait…
    </div>

    <div id="PageInnerSubFormContent" style="display: none;">
        <div class="select-message">
            <img src="../../Content/admin/img/information.png" /><br />
            Please select to Edit Or Add New
        </div>

    </div>

    <div id="PageInnerSubFormContent" style="display: none;">
        <div class="select-message">
            <img src="../../Content/admin/img/information.png" /><br />
            Please select to Edit Or Add New
        </div>

    </div>



    <script type="text/javascript" src="~/Content/admin/lib/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Content/admin/lib/js/bootstrap.min.js"></script>

    <script src="~/Content/admin/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Content/admin/js/moment.js"></script>
    <script type="text/javascript" src="~/Content/admin/lib/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Content/admin/lib/js/dataTables.bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/admin/lib/js/select2.full.min.js"></script>
    <script src="~/Content/admin/js/jquery.dataTables.js"></script>
    <script src="~/Content/admin/js/datatable-datetime.js"></script>
    <script src="~/Content/admin/js/datatable-currency.js"></script>
    <script src="~/Content/admin/js/semantic.min.js"></script>
    <script src="~/Scripts/modal.min.js"></script>
    <script src="~/Scripts/Chart.bundle.min.js"></script>

    <script src="~/Content/admin/js/azync_helper.js"></script>
    <script src="~/Content/admin/js/common.js"></script>
    <script src="~/Content/admin/js/delay_callprovider.js"></script>
    <script src="~/Content/admin/js/Forms/Trainer_Frm.js"></script>
    <script src="~/Content/admin/js/Forms/FreeWorkout_Frm.js"></script>
    <script src="~/Content/admin/js/Forms/Rate_Frm.js"></script>
    <script src="~/Content/admin/js/Forms/Order_Frm.js"></script>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyACH7n6OjMwjmKM0sRGfhXd3X_prW7aaJ4&libraries=visualization,drawing,places,weather&callback=initMap"
            async defer></script>

    @RenderSection("Scripts", required: false)
    <script>
        (function ($) {
            $(window).on("load", function () {

                //$(".mCustomScrollbar").mCustomScrollbar({
                //    theme: "minimal"
                //});

            });
        })(jQuery);
    </script>
</body>
</html>
